{
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "scripts": {
    "test": [
      "@phpcs",
      "./vendor/bin/phpunit tests"
    ],
    "phpcs": "php-cs-fixer fix"
  }
}
